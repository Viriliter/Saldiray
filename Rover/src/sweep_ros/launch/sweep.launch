<launch>
  <node pkg="sweep_ros" type="list.py" name="LIDAR2" output="screen" launch-prefix="xterm -e"/>
  <node name="sweep_node"	pkg="sweep_ros"  type="sweep_node" output="screen" launch-prefix="xterm -e">
  	<param name="serial_port"         type="string" value="/dev/sweep"/>
  	<param name="serial_baudrate"     type="int"    value="115200"/>
  	<param name="frame_id"            type="string" value="laser_frame"/>
  	<param name="rotation_speed"	  type="int" value="5"/>
  </node>
</launch>
