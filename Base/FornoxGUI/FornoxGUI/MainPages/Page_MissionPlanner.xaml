                <Label x:Name="Lbl_TotalDistance" Content="Total Distance" Margin="0,100,0,0"  HorizontalContentAlignment="Center" VerticalAlignment="Top" Height="40"  Foreground="White" FontSize="20" FontWeight="Bold"/>
                <TextBlock x:Name="Text_Distance"  Margin="0,140,0,0" VerticalAlignment="Top" Height="40" Foreground="White" FontSize="18" FontWeight="Bold" HorizontalAlignment="Center"/>
                <Frame x:Name="ElevationPlot" Margin="5,180,5,5" Height="300"/>
            </Grid>
            <Expander.Style>
                <Style TargetType="Expander">
                    <Setter Property="Opacity" Value="0.5"/>
                    <Style.Triggers>
                        <Trigger Property="IsMouseOver" Value="True">
                            <Setter Property="Opacity" Value="1"/>
                        </Trigger>
                    </Style.Triggers>
                </Style>
            </Expander.Style>
        </Expander>

        <Grid Margin="0,150,0,0">
            <Button x:Name="Btn_SwitchView" ToolTip="Switch Map View" Margin="10,0,0,0" HorizontalAlignment="Left" VerticalAlignment="Top" Click="Btn_SwitchView_Click">
                <ContentControl Content="{StaticResource MapIcon}" Margin="1"/>
                <Button.Template>
                    <ControlTemplate x:Name="Btn" TargetType="Button" >
                        <Grid >
                            <Path Stretch="UniformToFill" UseLayoutRounding="True" Fill="DimGray">
                                <Path.Data>
                                    <EllipseGeometry RadiusX="1" RadiusY="1"/>
                                </Path.Data>
                            </Path>
                            <ContentPresenter HorizontalAlignment="Center" VerticalAlignment="Center"/>
                        </Grid>
                    </ControlTemplate>
                </Button.Template>
                <Button.Style>
                    <Style TargetType="Button">
                        <Setter Property="Opacity" Value="0.5"/>
                        <Setter Property="Width" Value="40"/>
                        <Setter Property="Height" Value="40"/>
                        <Style.Triggers>
                            <Trigger Property="IsMouseOver" Value="True">
                                <Setter Property="Opacity" Value="1"/>
                                <Setter Property="Width" Value="50"/>
                                <Setter Property="Height" Value="50"/>
                            </Trigger>
                        </Style.Triggers>
                    </Style>
                </Button.Style>
            </Button>

            <Button x:Name="Btn_CreateRoute" ToolTip="Create Route" Margin="10,80,0,0" HorizontalAlignment="Left" VerticalAlignment="Top" Click="Btn_CreateRoute_Click">
                <ContentControl Content="{StaticResource VectorPolylineIcon}" Margin="0" />
                <Button.Template>
                    <ControlTemplate TargetType="Button" >
                        <Grid >
                            <Path Stretch="Uniform" UseLayoutRounding="False" Fill="DimGray">
                                <Path.Data>
                                    <EllipseGeometry RadiusX="1" RadiusY="1"/>
                                </Path.Data>
                            </Path>
                            <ContentPresenter HorizontalAlignment="Center" VerticalAlignment="Center"/>
                        </Grid>
                    </ControlTemplate>
                </Button.Template>
                <Button.Style>
                    <Style TargetType="Button">
                        <Setter Property="Opacity" Value="0.5"/>
                        <Setter Property="Width" Value="40"/>
                        <Setter Property="Height" Value="40"/>
                        <Style.Triggers>
                            <Trigger Property="IsMouseOver" Value="True">
                                <Setter Property="Opacity" Value="1"/>
                                <Setter Property="Width" Value="50"/>
                                <Setter Property="Height" Value="50"/>
                            </Trigger>
                        </Style.Triggers>
                    </Style>
                </Button.Style>
            </Button>

            <Button x:Name="Btn_GetElevationData" ToolTip="Show Elevation Data" Margin="10,160,0,0" HorizontalAlignment="Left" VerticalAlignment="Top" Click="Btn_GetElevationData_Click">
                <ContentControl Content="{StaticResource BufferIcon}" Margin="-1" />
                <Button.Template>
                    <ControlTemplate TargetType="Button" >
                        <Grid >
                            <Path Stretch="Uniform" UseLayoutRounding="False" Fill="DimGray">
                                <Path.Data>
                                    <EllipseGeometry RadiusX="1" RadiusY="1"/>
                                </Path.Data>
                            </Path>
                            <ContentPresenter HorizontalAlignment="Center" VerticalAlignment="Center"/>
                        </Grid>
                    </ControlTemplate>
                </Button.Template>
                <Button.Style>
                    <Style TargetType="Button">
                        <Setter Property="Opacity" Value="0.5"/>
                        <Setter Property="Width" Value="40"/>
                        <Setter Property="Height" Value="40"/>
                        <Style.Triggers>
                            <Trigger Property="IsMouseOver" Value="True">
                                <Setter Property="Opacity" Value="1"/>
                                <Setter Property="Width" Value="50"/>
                                <Setter Property="Height" Value="50"/>
                            </Trigger>
                        </Style.Triggers>
                    </Style>
                </Button.Style>
            </Button>
        </Grid>
    </Grid>
</Page>
